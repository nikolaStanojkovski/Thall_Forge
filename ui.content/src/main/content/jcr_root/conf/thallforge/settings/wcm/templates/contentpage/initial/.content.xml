<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            jcr:primaryType="cq:PageContent"
            sling:resourceType="thallforge/pages/contentpage"
            cq:template="/conf/thallforge/settings/wcm/templates/contentpage">
        <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="thallforge/components/structure/header"/>
        <main
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"/>
        <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="thallforge/components/structure/footer"/>
    </jcr:content>
</jcr:root>
