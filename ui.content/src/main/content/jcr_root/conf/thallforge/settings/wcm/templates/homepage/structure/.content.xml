<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="thallforge/pages/homepage"
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/thallforge/settings/wcm/templates/homepage"
        cq:lastReplicated="{Date}2019-02-27T16:37:23.968+01:00"
        cq:lastReplicatedBy="admin"
        cq:lastReplicationAction="Activate">
        <main
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"
            editable="{Boolean}true">
            <cq:responsive jcr:primaryType="nt:unstructured"/>
        </main>
        <latestreleases
                jcr:primaryType="nt:unstructured"
                sling:resourceType="thallforge/components/structure/latestreleases"
                editable="{Boolean}true">
        </latestreleases>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1100"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
